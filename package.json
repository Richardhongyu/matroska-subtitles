{
  "name": "matroska-subtitles",
  "version": "3.2.0",
  "description": "Streaming parser for embedded .mkv subtitles.",
  "source": "src/index.js",
  "main": "dist/matroska-subtitles.js",
  "module": "dist/matroska-subtitles.module.js",
  "dependencies": {
    "ebml": "^3.0.0",
    "ebml-block": "^1.1.2",
    "readable-stream": "^3.6.0"
  },
  "devDependencies": {
    "dev-null": "^0.1.1",
    "microbundle": "^0.12.4",
    "standard": "*",
    "terser": "^5.3.8",
    "wzrd": "^1.5.0"
  },
  "scripts": {
    "test": "standard && node tests/test.js",
    "build": "microbundle --name=MatroskaSubtitles",
    "bundle": "browserify --standalone MatroskaSubtitles . | terser --compress --source-map --output=dist/matroska-subtitles.min.js",
    "web": "wzrd examples/web.js",
    "prepare": "npm run build && npm run bundle",
    "prepublishOnly": "npm test"
  },
  "keywords": [
    "mkv",
    "srt",
    "matroska",
    "subtitle",
    "extract",
    "parse"
  ],
  "author": "Mathias Rasmussen <mathiasvr@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mathiasvr/matroska-subtitles.git"
  },
  "bugs": {
    "url": "https://github.com/mathiasvr/matroska-subtitles/issues"
  },
  "homepage": "https://github.com/mathiasvr/matroska-subtitles#readme",
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "files": [
    "dist"
  ]
}
